[ {
  "name" : "default",
  "description" : "Default configuration",
  "configuration" : {
    "nonPause" : false,
    "cloudRegionId" : 4,
    "podAssignPolicy" : {
      "selector" : {
        "label" : "cloud-pipeline/d4sv3",
        "value" : "true"
      },      
      "skipContainerRequests" : "false"
